{
  "name": "silentsos",
  "version": "1.0.0",
  "description": "Real-time emergency detection web application",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "keywords": [
    "emergency",
    "detection",
    "real-time",
    "safety"
  ],
  "author": "SilentSOS Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "dependencies": {
    "react-scripts": "^5.0.1"
  }
}
